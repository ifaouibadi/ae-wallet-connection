{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@ae-wallet-connection/features/ae-sdk": [
        "libs/features/ae-sdk/src/index.ts"
      ],
      "@ae-wallet-connection/features/aepp-communication/shared": [
        "libs/features/aepp-communication/shared/src/index.ts"
      ],
      "@ae-wallet-connection/features/aepp-communication/wallet-beacon": [
        "libs/features/aepp-communication/wallet-beacon/src/index.ts"
      ],
      "@ae-wallet-connection/features/aepp-communication/wallet-connect": [
        "libs/features/aepp-communication/wallet-connect/src/index.ts"
      ],
      "@ae-wallet-connection/features/async-storage": [
        "libs/features/async-storage/src/index.ts"
      ],
      "@ae-wallet-connection/features/chakra-ui": [
        "libs/features/chakra-ui/src/index.ts"
      ],
      "@ae-wallet-connection/ui/components": [
        "libs/ui/components/src/index.ts"
      ],
      "@ae-wallet-connection/ui/dapp": ["libs/ui/dapp/src/index.ts"],
      "@ae-wallet-connection/ui/wallet": ["libs/ui/wallet/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
